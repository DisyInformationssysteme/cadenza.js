{
  "name": "cadenza-public-api",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "run-p build:*",
    "build:docs": "typedoc",
    "build:ts": "tsc",
    "reformat": "prettier ./src --write",
    "start": "run-p start:*",
    "start:docs": "npm run build:docs -- --watch",
    "start:test": "npm test -- --watch",
    "start:reformat": "onchange \"src/**/*\" -- prettier {{changed}} --write",
    "test": "cross-env NODE_OPTIONS=--unhandled-rejections=warn jest"
  },
  "devDependencies": {
    "@types/jest": "29.5.3",
    "cross-env": "7.0.3",
    "jest": "29.6.1",
    "jest-environment-jsdom": "29.6.1",
    "npm-run-all": "4.1.5",
    "onchange": "7.1.0",
    "prettier": "3.0.0",
    "ts-jest": "29.1.1",
    "typedoc": "0.24.8"
  },
  "engines": {
    "node": "16.13.1"
  },
  "volta": {
    "node": "16.13.1"
  }
}
